<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.readymix">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Readymix">
        <activity android:name=".copy_event" />
        <activity android:name=".calender" />
        <activity android:name=".user_logging" />
        <activity android:name=".verify" />
        <activity android:name=".sign_up" />
        <activity android:name=".current_user" />
        <activity android:name=".choose" />
        <activity android:name=".admin_log" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>